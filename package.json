{
  "name": "eaf-ui",
  "version": "0.0.10",
  "private": true,
  "main": "lib/hd.common.js",
  "scripts": {
    "local": "node --max_old_space_size=4096 node_modules/@vue/cli-service/bin/vue-cli-service.js serve",
    "start": "npm run serve --open",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "clean": "rimraf lib && rimraf packages/*/lib && rimraf test/**/coverage",
    "build:file": "node build/bin/build-entry.js",
    "build:utils": "cross-env BABEL_ENV=utils babel src --out-dir lib/lib --ignore src/index.js",
    "build:theme": "gulp build --gulpfile build/gulpfile.js && node build/bin/copy-assets.js",
    "dist": "npm run clean && npm run build:file && node build/bin/copy-packages.js && npm run build:utils && webpack --mode development --config build/webpack.common.js && npm run build:theme"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "echarts": "^4.1.0",
    "element-ui": "^2.4.3",
    "highlight": "^0.2.4",
    "highlight.js": "^9.12.0",
    "sortablejs": "^1.7.0",
    "velocity-animate": "^1.5.2",
    "vue": "^2.5.16",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-plugin-unit-jest": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "@vue/eslint-config-prettier": "^3.0.1",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-module-resolver": "^2.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.8.0",
    "babel-plugin-transform-vue-jsx": "^3.3.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.5.0",
    "eslint-config-elemefe": "^0.3.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-html": "^4.0.5",
    "eslint-plugin-json": "^1.2.1",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^6.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-less": "^4.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "json-templater": "^1.2.0",
    "less": "^3.7.1",
    "less-loader": "^4.1.0",
    "progress-bar-webpack-plugin": "^1.11.0",
    "rimraf": "^2.6.2",
    "style-loader": "^0.23.0",
    "uppercamelcase": "^3.0.0",
    "vue-template-compiler": "^2.5.17",
    "webpack-chain": "^4.8.0",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
